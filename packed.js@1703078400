    body.on('success', ".set_news_readed", function(e, data){
//        $(this).parents('.hentry').find('.entry-title').append('<i class="icon-eye-open"></i>');
        $(this).parents('.hentry').addClass('viewed');
    });

    body.on('replace', ".set_focus", function(){
        $(this).find('textarea:visible, input:visible').first().caret_to_end();
    });

