//# sourceMappingURL=ZeroClipboard.min.map
function google_tag_manager ($trackingOptions) {
    let gtm_id = $('body').data('edu_settings').GTM_ID
    if (gtm_id === '') return;
    (function (w, d, s, l, i) {
        w[l] = w[l] || []
        w[l].push({
            'gtm.start':
              new Date().getTime(), event: 'gtm.js'
        })
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''
        j.async = true
        j.src =
          'https://www.googletagmanager.com/gtm.js?id=' + i + dl
        f.parentNode.insertBefore(j, f)
    })(window, document, 'script', 'dataLayer', gtm_id)
    window.dataLayer.push({ event: 'PageView' })
}

$(function () {
    var $trackingOptions = $('.tracking_options')
    if ($trackingOptions.data('metrica') && $trackingOptions.data('metrica') == 'True') {
        google_tag_manager($trackingOptions)
    }
})

!function ($) {

    "use strict"; // jshint ;_;

    var TextAreaAutoSize = function (element, options) {
        this.init('text_area_auto_size', element, options)
    };

    TextAreaAutoSize.prototype = {

        constructor: TextAreaAutoSize

        , init: function (type, element, options) {
            this.type = type;
            //this.element = element;
            this.$element = $(element);
            this.options = this.getOptions(options);
            this.$element.before('<div class="comment_text_hidden break_words" style="visibility: hidden; position: absolute; line-height: 20px;"></div>');
            this.helper = this.$element.prev();
            var _this = this;
            var line_height = parseInt(this.$element.css('line-height'));
            this.$element.on('keyup click', function(e, data){
                var text = '';
                var rows = _this.$element.value().replace(/[<>]/g, '_').split("\n");
                $.each(rows, function(index, s){
                    text = text + '<div>' + s.replace(/\s\s/g, ' &nbsp;') + '&nbsp;</div>'+"\n";
                });
                _this.helper.html(text).width(_this.$element.width());
                _this.$element.height(Math.max(_this.helper.outerHeight(), line_height));
            });
            this.$element.trigger('keyup')
        }

        , getOptions: function (options) {
            options = $.extend({}, $.fn[this.type].defaults, this.$element.data(), options);
            return options
        }
    };

    var old = $.fn.text_area_auto_size;

    $.fn.text_area_auto_size = function ( option ) {
        return this.each(function () {
            var $this = $(this), data = $this.data('text_area_auto_size') , options = typeof option == 'object' && option;
            if (!data) $this.data('text_area_auto_size', (data = new TextAreaAutoSize(this, options)));
            if (typeof option == 'string') data[option]()
        })
    };
    $.fn.text_area_auto_size.Constructor = TextAreaAutoSize;
    $.fn.text_area_auto_size.defaults = {};
    $.fn.text_area_auto_size.noConflict = function () {
        $.fn.text_area_auto_size = old;
        return this
    }

}(window.jQuery);


$(function() {
    function autosize_textarea(e) {
        $(e).find(".textarea-autosize").text_area_auto_size();
    }

    $.fn.register_preparer(autosize_textarea, true);
});