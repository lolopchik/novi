    // });

//### Tabs ###//
    body.on('success', ".get_tab_btn", function(e, data){
        if (!data.html) return;
        var $this = $(this);
        $this.addClass('loaded');
        var tab_content = $this.parents('.nav:first').nextAll('.tab-content:first');
        var related_content = tab_content.find('.tab-pane[data-name="'+ $this.data('tab_name') + '"]');
        tab_content.find('> .tab-pane').removeClass('active');
        related_content.addClass('active');
        related_content.replace_core($this.data('replace-method') || 'html', data.html);
    });

    body.on('click', ".get_tab_btn.loaded", function(e, data){
        var $this = $(this);
        var tab_content = $this.parents('.nav:first').nextAll('.tab-content:first');
        tab_content.find('> .tab-pane').removeClass('active');
        var related_content = tab_content.find('.tab-pane[data-name="'+ $this.data('tab_name') + '"]');
        related_content.addClass('active');
    });

    body.on('append_data', ".get_tab_btn.loaded", function(e, data){
       if (!$(this).hasClass('refresh_always')) data['loaded']= true;
       return data;
    });
    body.on('append_data', ".get_tab_btn", function(e, data){
        var $this = $(this);
        if ($this.data('tab_params'))
            $.fn.merge_recursive(data, $this.data('tab_params'));
        $this.data('tab_params', "")
    });

    body.on('click', ".click_tab", function(e, data){
        var $this = $(this);
        var tab_name = $this.data('tab_name');
        var tab = $this.parents('.extra-tabs:first').find('.nav li a[data-tab_name="' + tab_name + '"]');
        if ($this.data('tab_params'))
            tab.data('tab_params', $this.data('tab_params'));
        tab.click();
    });

//### Toogle content ###//
    function toggle_accordion_with_memory($this) {
        var content = $this.nextAll('.toggle_content:first');
        content.toggleClass( "activated");
    }

//### Ajax toogle content ###//

    body.on('success', ".ajax_toggle_btn", function(e, data){
        if (!data.html || this != e.target)
            return;
        var $this = $(this);
        $this.addClass('loaded');
        var related_content = eval($this.data('content-selector'));
        related_content.replace_core('html', data.html);
    });


    body.on('append_data', ".ajax_toggle_btn", function(e, data){
        var $this = $(this);
        toggle_accordion_with_memory($this);
        var related_content = $this.nextAll('.toggle_content:first');
        if (related_content.hasClass('activated')) data['opened'] = 'opened';
        return data;
    });

    body.on('append_data', ".ajax_toggle_btn.loaded", function(e, data){
        if (!$(this).hasClass('refresh_always')) data['loaded']= true;
        return data;
    });

    body.on('append_data', ".append_additional_data", function(e, data){
        $.extend(data, $(this).data('additional-data'));
    });

    body.on('append_data', ".add_rating", function(e, data){
       data['complexity'] = $(this).val();
    });

    body.on('change', ".add_rating", function(e, data){
        $(this).click();
    });

//### Keyboard default events ###//
    body.on('keypress', ".submit-by-enter .submit-on-change, .fast_submit", function(e, data){
        var key_code = (e.keyCode ? e.keyCode : e.which);
        if (e.ctrlKey && (e.keyCode == 13 || e.keyCode == 10) && this.localName == 'textarea' ||
            key_code == 13 && this.localName == 'input') {
            var form = $(this).parents('.form:first, form:first').first();
            var fast_elements = form.find(".submit-by-enter .submit-on-change, .fast_submit");
            if (fast_elements.index(this) + 1 < fast_elements.length)
                $(fast_elements[fast_elements.index(this) + 1]).caret_to_end();
            else
                form.find('.btn-main:first').click();
        }
        else if(key_code == 27) {
            $(this).parents('.page-content:first, .form:first, form:first').first().find('.btn-cancel:first').click();
        }
    });

    body.on('change', '.submit-by-change .submit-on-change', function(e, data){
        $(this).parents('.form:first').find('.btn-primary').click();
    });
//### Video thumbnail to player ###//
    body.on('click', ".video_thumbnail", function (e, data) {
        var container = $(this).parents('.video_container:first');
        container.replaceWith($(this).data('iframe-code'))
    });


//### Popover auto close ###//
    $.fn._popover_to_close = [];
    $(window).resize(function () {
        $($.fn._popover_to_close).each(function(){
            close_popover(this);
        });
        $.fn._popover_to_close.splice(0, $.fn._popover_to_close.length);
    });

    body.on('shown.bs.popover', '.get-popover-btn', function(e, data){
        e.preventDefault();
        var popover_data = $(this).data('bs.popover');
        if (popover_data && popover_data != undefined) {
            var tip = popover_data.tip();
            var is_exists_index = $.fn._popover_to_close.indexOf(tip);
            if ( is_exists_index == -1){
                $.fn._popover_to_close.push(tip);
            }
        }
        return false
    });

    body.on('hidden.bs.popover', '.get-popover-btn', function(e, data){
        e.preventDefault();
        var popover_data = $(this).data('bs.popover');
        if (popover_data && popover_data != undefined) {
            var tip = popover_data.tip();
            var idx = $.fn._popover_to_close.indexOf(tip);
            if (idx != -1) {
                $.fn._popover_to_close.splice(idx, 1);
            }
        }
        return false
    });

//### ADD dates ###//
    body.on('append_data', ".add_opened_tabs", function(e, data){
        var date_elements = $(this).parents('.page-content:first').find('.has_tab_value');
        var opened_dates = [];
        var closed_dates = [];
        $.each(date_elements, function(index, element){
            var $element = $(element);
            var push_to = closed_dates;
            if ($element.height() > 0)
                push_to = opened_dates;
            push_to.push($element.data('tab_value'))
        });
        data['opened_items'] = opened_dates.join();
        data['closed_items'] = closed_dates.join();
    });

//### file-input behaviour ###//
    body.on('change', '.file-input-wrapper input[type=file]', function(e, data){
        var fileName = $(this).val();
        $(this).parent().next('.file-input-name').remove();
        if (!!$(this).prop('files') && $(this).prop('files').length > 1) {
            fileName = $(this)[0].files.length + ' files';
        } else {
            fileName = fileName.substring(fileName.lastIndexOf('\\') + 1, fileName.length);
        }
        $(this).parent().after('<span class="file-input-name">' + fileName + '</span>');
    });

    body.on('append_data', '.refresh-timestamp', function(e, data) {
        var $this = $(this);
        var holder = eval($this.data('replaceSelector'));
        data['timestamp'] = holder.find('.timestamp').val();
    });

    body.on('success', ".refresh-timestamp", function(e, data){
        var $this = $(this);
        var holder = eval($this.data('replaceSelector'));
        holder.find('.timestamp').val(data['timestamp'])
    });


    body.on('click', '.pagination-scroll', function(e, data){
        var $this = $(this);
        var holder = body;
        if ($this.parents().hasClass('modal')) {
            holder = $('.modal');
        }
        holder.scrollTo($this.parents('.ajax-table-content:first'), {offset: -50});
    });

    body.on('before_submit', '.from-suffix-btn', function(e, data){
        var $this = $(this);
        var parent = undefined;
        if ($this.data('suffix-from'))
            parent = eval($this.data('suffix-from'));
        else {
            parent = $this.closest('.page-content');

            if (!parent.data('url')) parent = parent.find('.refresh-btn:first');
        }
        let suffix = $this.data('suffix') || ''
        var parent_url = window.location;
        if (suffix.length && suffix[0] == '/') parent_url = ''
        else if (parent.length) parent_url = parent.data('url');
        if ($this.data('add-row-id'))
            parent_url += $this.closest('.item-row, .preview-row' ).data('pk') + "/";
        $this.data('ajax_button').options.url = parent_url + suffix;
        $this.removeClass('from-suffix-btn')
    });

    body.on('click', '.suffix-link', function(e, data){
        var $this = $(this);
        var redirect = $this.closest('.page-content').data('url') + $this.data('suffix');
        if (redirect) location.href = redirect;
    });

    function scroll(element, offset){
        $.scrollTo($(element), 400, {offset: offset || -50})
    }

    function _scroll_to($this){
        scroll($($this.data('scroll-to') || $this.attr('href')), parseInt($this.data('scroll-offset')));
    }
    body.on('shown', '.scroll-to-tab', function(e){_scroll_to($(this))});
    body.on('click', '.active  .scroll-to-tab', function(e){_scroll_to($(this))});
    body.on('click', '.scroll-to', function(e){_scroll_to($(this))});
    body.on('replace-success', '.scroll-to-on-success', function(e){
        var $this = $(this);
        scroll(eval($this.data('scroll-to')), $this.data('scroll-offset'))
    });
    body.on('elapsed', '.standing .countdown-begin', function(e, datadata) {
        $(this).trigger('refresh-content');
    });
// ### FORM
    body.on('before_submit', '.btn-drop-form', function(e){
        var form = $(this).parents('form, .form').first();
        form.find('input[type!=hidden]').val("");
    });

    body.on('success', '.refresh-etalon', function (e) {
        $(this).parents('.page-content.problem:first').find('.etalon_runs').trigger('refresh-content');
    });

    body.on('modal-close', '.refresh-runs', function (e) {
        $(this).parents().find('.page-content.problem-runs:first').trigger('refresh-content');
    });

    body.on('modal-close', '.refresh-etalon-after-modal', function (e, data) {
        $(this).parents('.etalon_runs').trigger('refresh-content');
    });

    body.on('before_submit', '.lock_view', function(e) {
       $(this).closest('.page-content').find('.ajax_btn').not(this).addClass('disabled').addClass('locked_view')
    });
    body.on('success', '.lock_view', function(e) {
       $(this).closest('.page-content').find('.ajax_btn.locked_view').removeClass('disabled').removeClass('locked_view')
    });
    body.on('failed', '.lock_view', function(e) {
       $(this).closest('.page-content').find('.ajax_btn.locked_view').removeClass('disabled').removeClass('locked_view')
    });
    body.on('success', '.set_jwt', function(e, data) {
      localStorage.setItem('jwt_token', data.token)
      $(this).reload_page(e)
    });

    body.on('show.bs.dropdown', '.append_to_diary', function (e) {
        var dropdownMenu = $(e.target).find('.dropdown-menu');
        var diary_root = $('.diary2');
        diary_root.append(dropdownMenu.detach());
        $(e.target).append('<div class="dropdown-menu dropdown-menu-fake"></div>')
        var eOffset = $(e.target).offset();
        var root_offset = diary_root.offset()
        dropdownMenu.css({
            'display': 'block',
            'top': eOffset.top + $(e.target).outerHeight() - root_offset.top,
            'left': eOffset.left - root_offset.left
        });
        $(e.target).data('dropdown_detached', dropdownMenu[0])
    });

    body.on('hide.bs.dropdown', '.append_to_diary', function (e) {
        $(e.target).find('.dropdown-menu-fake').remove()
        var dropdownMenu = $(e.target).data('dropdown_detached')
        if (!dropdownMenu) return
        dropdownMenu = $(dropdownMenu);
        $(e.target).append(dropdownMenu.detach());
        dropdownMenu.hide();
    });
});



(function($){
    $.fn.cancel_in_place = function() {
        $.each(this, function(index, e){
            e.cancel()
        })
        ;
    };
})(jQuery);

!function ($) {

    "use strict"; // jshint ;_;

    var InPlaceEditor = function (element, options) {
        this.init('in_place_editor', element, options)
    };


    function has_in_place(element) {
        return $.data(element, "has_in_place");
    }

    function set_in_place(element, value) {
        $.data(element, "has_in_place", value);
    }

    function show_editor_error(editor, error) {
        editor.popover({
            title: "",
            content: error,
            trigger: 'hover',
            template: '<div class="popover error-popover"><div class="arrow"></div><h3 class="popover-title"></h3><div class="popover-content"></div></div>',
            container:'body',
            placement: 'auto right'
        });
        if (editor.is(':hover')) editor.popover('show');
    }

    InPlaceEditor.prototype = {

        constructor: InPlaceEditor

        , init: function (type, element, options) {
            this.type = type;
            this.element = element;
            this.$element = $(element);
            this.options = this.getOptions(options);
            this.enabled = true;
            this.$element.on(this.options.trigger + "."+ this.type, $.proxy(this.get_editor, this));
            element._get_editor = $.proxy(this.get_editor, this);
            $.fn.in_place_editor.in_place_list.push(this.element);
        }

        , getOptions: function (options) {
            options = $.extend({}, $.fn[this.type].defaults, this.$element.data(), options);
            return options
        },

        get_editor: function (e) {
            if (!(e.target == this.element || this.options.include_child && e.target.parentNode == this.element ))
                return;
            if (this.options.prevent_condition && eval(this.options.prevent_condition))
                return;
            var $this = $(this.$element);
            if (has_in_place(this.element) || $this.find('.in-place-editor').length) return;
            set_in_place(this.element, true);
            var params = {
                type: this.options.method,
                url: this.options.url,
                data: {action: "get_editor"}
            };
            var widget = this;
            var done_function = function(data, textStatus, jqXHR) {
                if ($.fn.in_place_editor.in_place_list.indexOf(widget.element) < 0)
                    $.fn.in_place_editor.in_place_list.push(widget.element);
                var editor = $($.parseHTML(data.html.trim())[0]);
                editor.prepare_content();
                $this[widget.options.replace_method](editor);
                editor.caret_to_end();
                editor.trigger('editor_created', data);
                widget.bind_events(widget, editor);
            };

            if (widget.options.editor_html || widget.options.editor_method) {
                var html;
                if (widget.options.editor_method)
                    html = widget.options.editor_method(widget.$element);
                else
                    html = widget.options.editor_html;
                var data = {html: html};
                done_function(data);
            }
            else {
                $.ajax(params)
                    .done(done_function)
                    .fail(function(data, textStatus, jqXHR) {
                        $.fn.show_modal_error(data.status);
                    })
                    .always(function(data, textStatus, jqXHR) {
                        set_in_place(widget.element, false);
                    });
            }
        },

        submit_editor: function (on_fail) {
            if (this._locked) return;
            this._locked = true;
            var widget = this;
            var $this = $(this.$element);
            var editor = $this.find('.in-place-editor');
            if (!editor.length)
                editor = $this.siblings('.in-place-editor');
            var data = {
                value: editor.val(),
                    csrfmiddlewaretoken: $.cookie('csrftoken')
            };
            $.when(widget.$element.trigger('append_data', [data]))
            .done(function(){
                var params = {
                    type: 'POST',
                    url: widget.options.url,
                    data: data
                };
                $.ajax(params)
                    .done(function(data, textStatus, jqXHR) {
                        if (data.redirect) {
                            editor.removeClass(widget.options.error_cls);
                            $this.trigger('success_in_place', data);
                        }
                        else {
                            editor.addClass(widget.options.error_cls);
                            if (data.error != undefined) {

                                if (editor.data('bs.popover'))
                                    $.when(editor.popover('destroy')).done(show_editor_error(editor, data.error));
                                else
                                    show_editor_error(editor, data.error);
                                if (on_fail)
                                    on_fail()
                            }
                        }
                    })
                    .fail(function(data, textStatus, jqXHR) {
                        $.fn.show_modal_error(data.status);
                        if (on_fail)
                            on_fail();
                        else
                            editor.addClass(widget.options.error_cls);
                    }).always(function(data, textStatus, jqXHR) {
                        widget._locked = false;
                    });
            });
        },

        bind_events: function(widget, editor) {
            var cancel = function () {
                var in_place_editor = $(widget.element._in_place_editor);
                if (in_place_editor.data('bs.popover'))
                    in_place_editor.popover('destroy');
                widget.$element.trigger('cancel_in_place');
                widget.$element.off('.' + widget.type).removeData(widget.type);
                set_in_place(widget.element, false);
            };
            editor.keyup(function(event) {
                if(event.keyCode == 13) {
                    $.proxy(widget.submit_editor, widget)();
                }
                if(event.keyCode == 27) {
                    cancel();
                }
            });
            widget.element._in_place_editor = editor[0];
            widget.element.hide_in_place = function() {
                var editor = $(widget.element._in_place_editor);
                if (editor.length && editor.val().length > 0 && editor.is(':visible'))
                    $.proxy(widget.submit_editor, widget)(cancel);
                else
                    cancel()
            };
            widget.element.cancel = cancel;
            editor[0].hide_in_place = cancel;
        },

        cancel: function () {
            this.element.cancel();
        }

    };

    var old = $.fn.in_place_editor;

    $.fn.in_place_editor = function ( option ) {
        return this.each(function () {
            var $this = $(this)
                , data = $this.data('in_place_editor')

                , options = typeof option == 'object' && option;
            if (!data) $this.data('in_place_editor', (data = new InPlaceEditor(this, options)));
            if (typeof option == 'string') data[option]()
        })
    };

    $.fn.in_place_editor.Constructor = InPlaceEditor;

    $.fn.in_place_editor.in_place_list = [];

    $.fn.in_place_editor.defaults = {
        method: 'GET',
        replace_method: 'append',
        url: "",
        trigger: 'click',
        prevent_condition: '',
        error_cls: 'error-input',
        close_other: true,
        editor_html: "",
        editor_method: undefined,
        include_child: false
    };

    $.fn.in_place_editor.unlock = function (element) {
        var index = $.fn.in_place_editor.in_place_list.indexOf(element);
        if (index > -1)
            $($.fn.in_place_editor.in_place_list[index]).removeClass('lock');
    };


    $.fn.in_place_editor.noConflict = function () {
        $.fn.in_place_editor = old;
        return this
    }

}(window.jQuery);


$(function(){
    var body = $('body');

    body.on('click', ".has-in-place-editor", function(e, data){
        $(this).in_place_editor();
        this._get_editor(e);
    });

    body.click(function(e) {
        var target = e.target;
        var $target = $(target);
        if (!$target.is(':visible') && !$target.hasClass('in-place-content')) return;
        if (target.parentNode._in_place_editor && $target.parent().data('in_place_editor').options.include_child) {
            target = target.parentNode;
        }
        var editor_list = $.fn.in_place_editor.in_place_list;
        var not_removed_index = editor_list.length;
        $.each(editor_list, function (index, element){
            if (element != target && element._in_place_editor && element._in_place_editor != target &&
                element._in_place_editor.parentNode != target &&
                element._in_place_editor.parentNode != target.parentNode) {
                var $element = $(element);
                if (element.hide_in_place && !$element.hasClass('lock')){
                    $element.addClass('lock');
                    if ($target.attr("id") && $target.attr("id").indexOf('error') !== -1)
                        $element.refresh_page_content();
                     else
                         element.hide_in_place();
                }
            }
            else
                not_removed_index = index;
        });
    });
});

/*
    jQuery Masked Input Plugin
    Copyright (c) 2007 - 2015 Josh Bush (digitalbush.com)
    Licensed under the MIT license (http://digitalbush.com/projects/masked-input-plugin/#license)
    Version: 1.4.1
*/
!function(factory) {
    "function" == typeof define && define.amd ? define([ "jquery" ], factory) : factory("object" == typeof exports ? require("jquery") : jQuery);
}(function($) {
    var caretTimeoutId, ua = navigator.userAgent, iPhone = /iphone/i.test(ua), chrome = /chrome/i.test(ua), android = /android/i.test(ua);
    $.mask = {
        definitions: {
            "9": "[0-9]",
            a: "[A-Za-z]",
            "*": "[A-Za-z0-9]"
        },
        autoclear: !0,
        dataName: "rawMaskFn",
        placeholder: "_"
    }, $.fn.extend({
        caret: function(begin, end) {
            var range;
            if (0 !== this.length && !this.is(":hidden")) return "number" == typeof begin ? (end = "number" == typeof end ? end : begin, 
            this.each(function() {
                this.setSelectionRange ? this.setSelectionRange(begin, end) : this.createTextRange && (range = this.createTextRange(), 
                range.collapse(!0), range.moveEnd("character", end), range.moveStart("character", begin), 
                range.select());
            })) : (this[0].setSelectionRange ? (begin = this[0].selectionStart, end = this[0].selectionEnd) : document.selection && document.selection.createRange && (range = document.selection.createRange(), 
            begin = 0 - range.duplicate().moveStart("character", -1e5), end = begin + range.text.length), 
            {
                begin: begin,
                end: end
            });
        },
        unmask: function() {
            return this.trigger("unmask");
        },
        mask: function(mask, settings) {
            var input, defs, tests, partialPosition, firstNonMaskPos, lastRequiredNonMaskPos, len, oldVal;
            if (!mask && this.length > 0) {
                input = $(this[0]);
                var fn = input.data($.mask.dataName);
                return fn ? fn() : void 0;
            }
            return settings = $.extend({
                autoclear: $.mask.autoclear,
                placeholder: $.mask.placeholder,
                completed: null
            }, settings), defs = $.mask.definitions, tests = [], partialPosition = len = mask.length, 
            firstNonMaskPos = null, $.each(mask.split(""), function(i, c) {
                "?" == c ? (len--, partialPosition = i) : defs[c] ? (tests.push(new RegExp(defs[c])), 
                null === firstNonMaskPos && (firstNonMaskPos = tests.length - 1), partialPosition > i && (lastRequiredNonMaskPos = tests.length - 1)) : tests.push(null);
            }), this.trigger("unmask").each(function() {
                function tryFireCompleted() {
                    if (settings.completed) {
                        for (var i = firstNonMaskPos; lastRequiredNonMaskPos >= i; i++) if (tests[i] && buffer[i] === getPlaceholder(i)) return;
                        settings.completed.call(input);
                    }
                }
                function getPlaceholder(i) {
                    return settings.placeholder.charAt(i < settings.placeholder.length ? i : 0);
                }
                function seekNext(pos) {
                    for (;++pos < len && !tests[pos]; ) ;
                    return pos;
                }
                function seekPrev(pos) {
                    for (;--pos >= 0 && !tests[pos]; ) ;
                    return pos;
                }
                function shiftL(begin, end) {
                    var i, j;
                    if (!(0 > begin)) {
                        for (i = begin, j = seekNext(end); len > i; i++) if (tests[i]) {
                            if (!(len > j && tests[i].test(buffer[j]))) break;
                            buffer[i] = buffer[j], buffer[j] = getPlaceholder(j), j = seekNext(j);
                        }
                        writeBuffer(), input.caret(Math.max(firstNonMaskPos, begin));
                    }
                }
                function shiftR(pos) {
                    var i, c, j, t;
                    for (i = pos, c = getPlaceholder(pos); len > i; i++) if (tests[i]) {
                        if (j = seekNext(i), t = buffer[i], buffer[i] = c, !(len > j && tests[j].test(t))) break;
                        c = t;
                    }
                }
                function androidInputEvent() {
                    var curVal = input.val(), pos = input.caret();
                    if (oldVal && oldVal.length && oldVal.length > curVal.length) {
                        for (checkVal(!0); pos.begin > 0 && !tests[pos.begin - 1]; ) pos.begin--;
                        if (0 === pos.begin) for (;pos.begin < firstNonMaskPos && !tests[pos.begin]; ) pos.begin++;
                        input.caret(pos.begin, pos.begin);
                    } else {
                        for (checkVal(!0); pos.begin < len && !tests[pos.begin]; ) pos.begin++;
                        input.caret(pos.begin, pos.begin);
                    }
                    tryFireCompleted();
                }
                function blurEvent() {
                    checkVal(), input.val() != focusText && input.change();
                }
                function keydownEvent(e) {
                    if (!input.prop("readonly")) {
                        var pos, begin, end, k = e.which || e.keyCode;
                        oldVal = input.val(), 8 === k || 46 === k || iPhone && 127 === k ? (pos = input.caret(), 
                        begin = pos.begin, end = pos.end, end - begin === 0 && (begin = 46 !== k ? seekPrev(begin) : end = seekNext(begin - 1), 
                        end = 46 === k ? seekNext(end) : end), clearBuffer(begin, end), shiftL(begin, end - 1), 
                        e.preventDefault()) : 13 === k ? blurEvent.call(this, e) : 27 === k && (input.val(focusText), 
                        input.caret(0, checkVal()), e.preventDefault());
                    }
                }
                function keypressEvent(e) {
                    if (!input.prop("readonly")) {
                        var p, c, next, k = e.which || e.keyCode, pos = input.caret();
                        if (!(e.ctrlKey || e.altKey || e.metaKey || 32 > k) && k && 13 !== k) {
                            if (pos.end - pos.begin !== 0 && (clearBuffer(pos.begin, pos.end), shiftL(pos.begin, pos.end - 1)), 
                            p = seekNext(pos.begin - 1), len > p && (c = String.fromCharCode(k), tests[p].test(c))) {
                                if (shiftR(p), buffer[p] = c, writeBuffer(), next = seekNext(p), android) {
                                    var proxy = function() {
                                        $.proxy($.fn.caret, input, next)();
                                    };
                                    setTimeout(proxy, 0);
                                } else input.caret(next);
                                pos.begin <= lastRequiredNonMaskPos && tryFireCompleted();
                            }
                            e.preventDefault();
                        }
                    }
                }
                function clearBuffer(start, end) {
                    var i;
                    for (i = start; end > i && len > i; i++) tests[i] && (buffer[i] = getPlaceholder(i));
                }
                function writeBuffer() {
                    input.val(buffer.join(""));
                }
                function checkVal(allow) {
                    var i, c, pos, test = input.val(), lastMatch = -1;
                    for (i = 0, pos = 0; len > i; i++) if (tests[i]) {
                        for (buffer[i] = getPlaceholder(i); pos++ < test.length; ) if (c = test.charAt(pos - 1), 
                        tests[i].test(c)) {
                            buffer[i] = c, lastMatch = i;
                            break;
                        }
                        if (pos > test.length) {
                            clearBuffer(i + 1, len);
                            break;
                        }
                    } else buffer[i] === test.charAt(pos) && pos++, partialPosition > i && (lastMatch = i);
                    return allow ? writeBuffer() : partialPosition > lastMatch + 1 ? settings.autoclear || buffer.join("") === defaultBuffer ? (input.val() && input.val(""), 
                    clearBuffer(0, len)) : writeBuffer() : (writeBuffer(), input.val(input.val().substring(0, lastMatch + 1))), 
                    partialPosition ? i : firstNonMaskPos;
                }
                var input = $(this), buffer = $.map(mask.split(""), function(c, i) {
                    return "?" != c ? defs[c] ? getPlaceholder(i) : c : void 0;
                }), defaultBuffer = buffer.join(""), focusText = input.val();
                input.data($.mask.dataName, function() {
                    return $.map(buffer, function(c, i) {
                        return tests[i] && c != getPlaceholder(i) ? c : null;
                    }).join("");
                }), input.one("unmask", function() {
                    input.off(".mask").removeData($.mask.dataName);
                }).on("focus.mask", function() {
                    if (!input.prop("readonly")) {
                        clearTimeout(caretTimeoutId);
                        var pos;
                        focusText = input.val(), pos = checkVal(), caretTimeoutId = setTimeout(function() {
                            input.get(0) === document.activeElement && (writeBuffer(), pos == mask.replace("?", "").length ? input.caret(0, pos) : input.caret(pos));
                        }, 10);
                    }
                }).on("blur.mask", blurEvent).on("keydown.mask", keydownEvent).on("keypress.mask", keypressEvent).on("input.mask paste.mask", function() {
                    input.prop("readonly") || setTimeout(function() {
                        var pos = checkVal(!0);
                        input.caret(pos), tryFireCompleted();
                    }, 0);
                }), chrome && android && input.off("input.mask").on("input.mask", androidInputEvent), 
                checkVal();
            });
        }
    });
});
$(function(){

    function create_croppie(e){
        requirejs(['jquery/plugins/croppie'], function(){
            _create_croppie(e);
        })
    }

    function  _create_croppie(e) {
        var $widget = $(e);
        var $input = $widget.find('.croppie-input');
        var $editor = $widget.find('.croppie-editor');
        var $btn = $widget.find('.croppie-btn');
        $editor.croppie($widget.data('config'));
        $editor.croppie('setZoom', 1.5);
        if ($widget.data('config')['url'])
            $editor.croppie('bind', {url: $widget.data('config').url, zoom: 1.5});


        $btn.on('click', function(){
            $input.click();
        });

        $widget.closest('.page-content').find('.btn-main').on('add_promises', function(e, promises, form_data){
            console.log('croppie serialize blob');
            form_data.append($widget.data('name'), $editor.croppie('result', {type: 'blob_now', size: $widget.data('config').size || 'viewport'}), 'croppie.png');
        });

        $input.on('change', function () {
            var input = this;
            if (input.files && input.files[0]) {
                var reader = new FileReader();

                reader.onload = function (e) {
                  $editor.croppie('bind', {url: e.target.result, zoom: 1.5});
                };

                reader.readAsDataURL(input.files[0]);
              }
        })
    }

    $.fn.run_after_render('croppie', create_croppie);
});


