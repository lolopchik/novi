//# sourceMappingURL=data:application/json;charset=utf-8;base64,

LOADER = '<div class="loader"><div></div></div>';

function enable_tooltips() {
    var body = $("body");

    function create_tooltips (e) {
        $(e).find('[rel=tooltip]:not(.has-tooltip)').map(function (index, _this) {
          var $this = $(_this).addClass('has-tooltip');
          var current_template = $this.data('template');
          var html = $this.data('tooltip-html') || false;
          var options = {
              template: $this.tooltip.template[current_template],
              trigger: $this.data('trigger'),
              html: html
          };
          var hide_delay = $this.data('hide-delay');
          if (hide_delay) options['delay'] = {'hide': hide_delay};
          $this.tooltip(options);
          $this.tooltip({container: '#tooltip_holder'});
        });
    }
    $.fn.register_preparer(create_tooltips, true);


    body.on('click','[rel=tooltip]',function(){
        if ($(this).data('bs.tooltip') && $(this).data('bs.tooltip').options.trigger != 'click')
            $(this).tooltip('hide');
    });
    body.on('hide.bs.tooltip','[rel=tooltip]',function(e){
        e.stopPropagation();
    });
    body.on('hidden.bs.tooltip','[rel=tooltip]',function(e){
        e.stopPropagation();
    });

    body.on('shown.bs.tooltip', '[rel=tooltip]',function(e){
        if (!$(this).data('bs.tooltip')) return
        var tip = $(this).data('bs.tooltip').tip();
        if (tip.length) tip[0].tooltip_to = $(this);
    });

    body.on("click", '.close-popover', function(e){
        var popover = $(this).parents(".popover:first");
        if (popover.data('popover_created_by') != undefined)
            popover.data('popover_created_by').popover('destroy');
        else
            $(this).parents(".popover:first").prev('[close-popover]:first').click();
    });
    body.on("shown.bs.popover", "[close-popover]", function(){
        var popover_title = $(this).next('.popover:first').find(".popover-title");
        popover_title.append('<button class="close close-popover" data-dismiss="popover" type="button"></button>');
    });

    body.on('click', '.popover_on_click', function (e) {
      var $this = $(this)
      var template = $this.data('template') || 'fixed_height_popover';
      var options = {
          html: true,
          template: $this.popover.template[template]
      };
      if ($this.data('popover')) return
      $this.addClass('has-popover').removeClass('popover_on_click');
      $this.popover('show')
    });

    $('.popover:has(div.arrow)').find('.popover-title').append('<button class="close" data-dismiss="alert" type="button"></button>');

    function register_wide_popover (e) {
      var $this = $(e);
      var template = $this.data('template') || 'wide_popover';
      var options = {
          html: true,
          template: $this.popover.template[template]
      };
      $this.addClass('has-popover').popover(options);
    }

    $.fn.run_after_render('register-wide-popover', register_wide_popover);

//  
    body.on('click', '.ajax_btn:not(.registered)', function(e, data){
        var $this = $(this);
        $this.addClass('registered');
        $this.ajax_button();
        this._submit_data(e, data);
    });

    body.on('click', '.invert-btn', function(event){
        var $element = $(this);
        var invert = $element.data('invert');
        var new_invert = $element.html();
        $($element).html(invert).data('invert', new_invert);
    });

    function click_other_buttons() {
        var $this = $(this);
        $this.addClass('disabled');
        var btn_list = eval($this.data('click-selector')) || [];
        if (!(btn_list instanceof Array))
            btn_list = [btn_list];
        var click_next_element = function(pos) {
            if (pos < btn_list.length) {
                btn_list[pos].click();
                $(btn_list[pos]).wait_ajax(function() {click_next_element(pos + 1) });
            }
            else {
                $this.removeClass('disabled');
                console.log('click finished')
            }

        };
        click_next_element(0);
    }

    body.on('success', '.ajax-other-btn-click', function(event, data){
        if (data.redirect != undefined)
            $.proxy(click_other_buttons, this)();
    });

    body.on('modal-close', '.after-modal-click', function(event) {
        $.proxy(click_other_buttons, this)();
    });

    body.on('click', '.click-other-btn', function(event) {
        $.proxy(click_other_buttons, this)();
    });

    body.on('change', '.click-on-change', function(event) {
        $.proxy(click_other_buttons, this)();
    });

    // 
    body.on('click, focusin', '.inline-ajax-search:not(.registered)', function(e){
        var $this = $(this);
        $this.addClass('registered');
        $this.autocomplete_widget();
    });

    body.on('close', '.error_alert', function(event, data) {
        $(this).parents('tr:first').remove();
    });

    var dropdown_index = 0;

    $.fn.update_dropdown = function(force) {

        function update_dropdown(holder) {
            var dropdown = $(holder);
            dropdown.addClass('updated');
            dropdown.each(function(index) {
                dropdown_index += 1;
                var element = $(this);
                var id = "dropdown-00" + dropdown_index + index;
                if (!element.attr('id') || force) element.attr('id', id);
                holder.nextAll(".dropdown-menu").attr('aria-labelledby', id)
            });
        }

        force = force || false;
        update_dropdown($(this));
    };

    function register_dropdown(e) {
        $(e).update_dropdown()
    }
    $.fn.run_after_render('register-dropdown', register_dropdown);

    body.on('click','.dropdown-onclick',function(e){
        if(e.target==this){
            e.stopPropagation();
            var dropdown = $(this).find('.dropdown-toggle');
            if (!dropdown.data('dropdown')) dropdown.dropdown();
            dropdown.focus();
            dropdown.trigger('click.dropdown.data-api');
        }
    });

    body.on('click','.set_dropdown_value',function(e){
       var $this = $(this);
        var menu = $this.parents('ul:first');
        menu.prevAll('input:first').value($this.value());
        menu.prevAll('a:first').html($this.html() + '<b class="caret">');
        $this.parents('form:first').find('a.ajax_btn').click()
    });

    body.on('click', '.presentation-btn', function(e){
        var $this = $(this);
        $this.next().one('before_submit', function(){
            $this.addClass('disabled');
        }).one('success', function(){
            $this.removeClass('disabled');
        }).click();
    });

    body.on('click', 'a.suffix_link', function(e){
        var $this = $(this);
        var url = $this.removeClass('suffix_link').closest('.page-content').data('url');
        $this.attr('href', url + $this.attr('href'));
    });

    body.on('click', '.export_xls', function(e){
        var $this = $(this);
        var table = $this.closest('.table-content, .page-content').find('table:first');
        table.find('tbody > tr > td').each(function () {
            var new_value = $(this).text($(this).text().toString().replace(/\./g, ','));
            $(this).text($(this).text().toString().replace(/^\d+\.\d+$/g, new_value));
        });
        requirejs(['jquery/plugins/excel_export', 'jquery/plugins/file_saver'], function(){
            var uri = table.battatech_excelexport({
                worksheetName: 'page',
                returnUri: true,
                returnFile: true
            });
             var blob = new Blob([uri], {type: "application/vnd.ms-excel;charset=charset=utf-8"});
            saveAs(blob, ($this.data('name') || "file") + ".xls");
        });
    });

    body.on("shown.bs.dropdown", ".dropdown:not(.dropdown-only)", function () {
        // calculate the required sizes, spaces
        var $this = $(this);
        var $ul = $this.children(".dropdown-menu");
        var $button = $this.children(".dropdown-toggle");
        var ulOffset = $ul.offset();
        // how much space would be left on the top if the dropdown opened that direction
        var spaceUp = (ulOffset.top - $button.height() - $ul.height()) - $(window).scrollTop();
        // how much space is left at the bottom
        var spaceDown = $(window).scrollTop() + $(window).height() - (ulOffset.top + $ul.height());
        // switch to dropup only if there is no space at the bottom AND there is space at the top, or there isn't either but it would be still better fit
        if (spaceDown < 0 && (spaceUp >= 0 || spaceUp > spaceDown))
          $this.addClass("dropup auto-dropup");
    }).on("hidden.bs.dropdown", ".dropdown", function() {
        if ($(this).hasClass('auto-dropup'))
            $(this).removeClass("dropup").removeClass('auto-dropup');
    });

    var current_uid = 0;
    $.fn.get_uid = function() {
        current_uid += 1;
        return "uid_" + String(current_uid);
    };

//    JS loader
    function load_scripts(script_list, success) {
        if (!script_list.length || script_list == undefined){
            success();
            return;
        }
        var prepared = script_list[0].replace(/\.[^/.]+$/, "");
        const script_path = '/static/' + prepared + '.js'
        if (($('head [data-requiremodule="' + prepared + '.js' +'"]')
            || $('head [data-requiremodule="' + script_path +'"]'))) {
            load_scripts(script_list.slice(1), success);
        } else {
            requirejs([script_path], function() {
                load_scripts(script_list.slice(1), success);
            })
        }
    }
    $.fn.load_js = function (script_name, success) {
        if (success == undefined) success = function(){};
        load_scripts(script_name, success);
    };

    $.fn.replace_object_by_function = function(obj, search_values, f) {
        for(var key in obj) {
            if (obj.hasOwnProperty(key)) {
                var value = obj[key];
                if ((!!value) && (value.constructor === Object))
                    $.fn.replace_object_by_function(value, search_values, f);
                else
                    if (search_values.indexOf(value) >= 0) obj[key] = f(value);
            }
        }

    };

}

if(!String.prototype.startsWith){
    String.prototype.startsWith = function (str) {
        return !this.indexOf(str);
    }
}

function a_link() {
    $(document).on("click", ".logout-link", function(e) {
        e.preventDefault();
        var href = $(this).data('url');
        if (href == undefined)
            href = $(this).attr('href');
        $("<form/>", {
            'action': href,
            'method': 'POST'
        }).hide().append($("<input>", {
                'name': 'csrfmiddlewaretoken',
                'value': $.cookie('csrftoken')
            })).appendTo('body').submit();
        localStorage.removeItem('jwt_token')
        localStorage.removeItem('jwt_token_expired_at')
    });
}

//first, checks if it isn't implemented yet
if (!String.prototype.format) {
  String.prototype.format = function() {
    var args = arguments;
    return this.replace(/{(\d+)}/g, function(match, number) {
      return typeof args[number] != 'undefined'
        ? args[number]
        : match
      ;
    });
  };
}

Number.prototype.pretty_format = function (number) {
    var result = this.toFixed(number)
    if (Math.abs(this - parseInt(result)) < 0.001) return Math.round(this)
    var index = result.length
    while (index > 0 && result[index - 1] === '0') index -= 1
    return result.substr(0, index)
}

String.prototype.as_date = function() {
    if (!moment)
        throw "Moment js required";
    return moment(this, "DD-MM-YYYY HH:mm:ss").toDate();
};

jQuery.fn.merge_recursive = function(arr1, arr2) {
    $.extend(true, arr1, arr2);
    return arr1;
};


jQuery.fn.outerHTML = function(s) {
    return s
        ? this.before(s).remove()
        : jQuery("<p>").append(this.eq(0).clone()).html();
};

jQuery.fn.clean_tooltips = function() {
    $.each($('.tooltip'), function(index, e){
        if (e.tooltip_to != undefined && !e.tooltip_to.closest('html').length){
            $(e).remove();
        }
    });
};

jQuery.fn.trim_html = function (s) {
    var start = s.indexOf('<');
    var end = s.lastIndexOf('>');
    if (start < 0) start = 0;
    if (end < 0) end = -1;
    return s.substring(start, end)
};


jQuery.expr.filters.offscreen = function(el) {
  var rect = el.getBoundingClientRect();
  return (
           (rect.x + rect.width) < 0
             || (rect.y + rect.height) < 0
             || (rect.x > window.innerWidth || rect.y > window.innerHeight)
         );
};

var picker_options = {
    "date":{
        format: 'L'
        //"pickTime": false
    },
    "time":{
        format: 'LT'
        //"pickDate": false
    },
    "datetime":{
        //"pickTime": true,
        //"pickDate": true
    }
};

icons = {
    time: 'icon-time',
    date: 'icon-calendar',
    up:   'icon-chevron-up',
    down: 'icon-chevron-down'
};

function enable_datepickers() {
    var body = $('body');
    body.on('click', '.datepickerbutton', function(){
        var $element = $(this);
        var picker = $element.closest('.input-group');
        var input = picker.find('input:first');
        var options = get_picker_options(input);
    if (!picker.data("DateTimePicker")){
        picker.datetimepicker(options).on('dp.change', function(e){
            input.attr('value', e.date.calendar());
            input.trigger('change')
        });
        picker.data("DateTimePicker").show()
    }
    });
}

function render_inline_datepicker(e) {
    var $this = $(e);
    var options = get_picker_options($this);
    options = $.extend({}, options, {
        inline: true,
        sideBySide: true
    });

    $this.datetimepicker(options);
    if ($this.data('colors')) {
        var colors = $this.data('colors');
        var offset = new Date().getTimezoneOffset() * - 60;
        $this.datetimepicker().on('dp.classify', function(e){
           if (colors.hasOwnProperty(e.date.unix()+offset))
                e.display.value = '<div class="relative '+ colors[e.date.unix()+offset] +'">'+ e.date.date() +'</div>';
        });
        $this.data('DateTimePicker').enabledDates([])
    }
    return $this;
}

function render_calendar_datepicker(e) {
    var $this = render_inline_datepicker(e);
    var page = $this.closest('.page-content');
    $this.datetimepicker().on('dp.change', function(e){
        var date = e.date;
        page.one('append_data', function(e, data){
            data['date_filter'] = date.format('DD.MM.YYYY')
        });
        page.refresh_page_content();

    });
}


function render_root_calendar_datepicker(element) {
    var $this = render_inline_datepicker(element);
    var page = $this.closest('.page-content');
    $this.datetimepicker().on('dp.change', function (e) {
        var date = e.date;
        page.one('append_data', function (e, data) {
            data['date_filter'] = date.calendar();
        });
        $.ajax({
            url: $this.data('url'),
            data: {
                date_filter: date.format('DD.MM.YYYY')
            }
        }).done(function(data, textStatus, jqXHR) {
            window.location = $this.data('url');
        });
    });
}

function get_picker_options(input) {
    var options = picker_options[$(input).data('picker-type') || 'date'];
    options.locale = $.fn.language_code;
    options.minDate = $.fn.get_moment(input.data('min-date'));
    options.maxDate = $.fn.get_moment(input.data('max-date'));
    if (input.data('default-date'))
        options.defaultDate = $.fn.get_moment(input.data('default-date'));
    options.showTodayButton = input.data('today-btn') != undefined;
    options.useCurrent = false;
    options.icons = icons;
    if (options.locale == 'ru')
        options.tooltips = picker_ru_tooltips;
    return options
}

picker_ru_tooltips = {
        today: 'Go to today',
        clear: ' ',
        close: '',
        selectMonth: ' ',
        prevMonth: ' ',
        nextMonth: ' ',
        selectYear: ' ',
        prevYear: ' ',
        nextYear: ' ',
        selectDecade: ' ',
        prevDecade: ' ',
        nextDecade: ' ',
        prevCentury: ' ',
        nextCentury: ' ',
        pickHour: ' ',
        incrementHour: ' ',
        decrementHour: ' ',
        pickMinute: ' ',
        incrementMinute: ' ',
        decrementMinute: ' ',
        pickSecond: ' ',
        incrementSecond: ' ',
        decrementSecond: ' ',
        togglePeriod: ' ',
        selectTime: ' '
};

function match_height(container) {
    requirejs(['jquery/plugins/jquery.matchHeight'], function(){
        //match_height
        var $container = $(container);
        var by_row = $container.data('byrow');
        if (by_row == undefined) by_row = true;
        $container.find($container.data('match')).matchHeight({byRow:  by_row})
    });
}

function render_peity(element) {
  requirejs(['jquery/plugins/jquery.peity'], function() {
    $(element).peity('donut');
  });
}


function yandex_share(element) {
    var $this = $(element);
    requirejs(['achievement/es5-shims.min', 'achievement/share'], function(){
        var share = Ya.share2(element, {
            content: {
                url: $this.data('url'),
                title: $this.data('title'),
                description: $this.data('description'),
                image: $this.data('image')
            },
            hooks: {
                onshare: function (name) {
                    var share_btn = this.element.next();
                    share_btn.click();
                }
            }
        });
        share.element = $this
    });
}


$(function(){
    enable_tooltips();
    a_link();
    var body = $('body');
    body.prepare_content();
    enable_datepickers();
    $.fn.refresh_token();
    body.set_focus();
    $.fn.run_after_render('inline-datepicker', render_inline_datepicker);
    $.fn.run_after_render('calendar-datepicker', render_calendar_datepicker);
    $.fn.run_after_render('root-calendar-datepicker', render_root_calendar_datepicker);
    $.fn.run_after_render('match_height', match_height);
    $.fn.run_after_render('peity', render_peity);
    $.fn.run_after_render('yandex_share', yandex_share);


    var _slider_confs = {
      slider_row: {
        dots: true,
        infinite: true,
        slidesToShow: 8,
        slidesToScroll: 8,
        focusOnSelect: false,
        accessibility: false,
        responsive: [
          {
            breakpoint: 1600,
            settings: {
              slidesToShow: 6,
              slidesToScroll: 6,
              infinite: true,
              dots: true
            }
          },
          {
            breakpoint: 1200,
            settings: {
              slidesToShow: 4,
              slidesToScroll: 4,
              infinite: true,
              dots: true
            }
          },
          {
            breakpoint: 800,
