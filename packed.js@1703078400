(function($) {

    $.fn.serializeAnything = function(callback) {
        var default_callback = function ($this){return $this.find(':input').get()};
        var element_getter = callback || default_callback;
        var toReturn = [];
        var els = element_getter($(this));

        $.each(els, function() {
            if (this.name && !this.disabled && (this.checked || /select|textarea/i.test(this.nodeName) || /email|text|hidden|number|time|password/i.test(this.type))) {
                var val = $(this).val();
                toReturn.push( encodeURIComponent(this.name) + "=" + encodeURIComponent(val));
            }
        });

        return toReturn.join("&");

    };

})(jQuery);

jQuery.fn.outerHTML = function(s) {
    return s
        ? this.before(s).remove()
        : jQuery("<p>").append(this.eq(0).clone()).html();
};
