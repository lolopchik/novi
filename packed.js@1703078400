,nextDay:"[明天]LT",nextWeek:"[下]ddddLT",lastDay:"[昨天]LT",lastWeek:"[上]ddddLT",sameElse:"L"},ordinalParse:/\d{1,2}(日|月|週)/,ordinal:function(a,b){switch(b){case"d":case"D":case"DDD":return a+"日";case"M":return a+"月";case"w":case"W":return a+"週";default:return a}},relativeTime:{future:"%s內",past:"%s前",s:"幾秒",m:"1 分鐘",mm:"%d 分鐘",h:"1 小時",hh:"%d 小時",d:"1 天",dd:"%d 天",M:"1 個月",MM:"%d 個月",y:"1 年",yy:"%d 年"}}),a.locale("en"),a});
(function($){
    var countdown_formatters = {
        long: function(duration) {
            var hours = new Date(0, 0, 0, duration.hours(), duration.minutes(), duration.seconds());
            var text =  moment(hours).format("HH:mm:ss");
            return moment.localeData().relativeTime(Math.floor(duration.asDays()), true, 'dd', true) + ' '+ text;

        },
        humanize: function(duration) {
            return duration.humanize()
        }
    };

    $.fn.countdown = function (server_time, format){
        var element = $(this);
        var interval_id = null;
        var timer = element.find('.timer:first');
        var until = element.data('date') || element.data('end_at');
        if (!until) return;
        var eventTime = until.as_date().getTime();
        var diffTime = eventTime - server_time;
        var duration = moment.duration(diffTime, 'milliseconds');
        var interval = 1000;
        var elapsed = false;
        format = format || 'long';
        var formatter = countdown_formatters[format];
        function update_countdown(){
            duration = moment.duration(duration - interval, 'milliseconds');
            if (duration._milliseconds < 0 && !elapsed){
                elapsed = true;
                clearInterval(interval_id);
                element.addClass('elapsed').trigger('elapsed');
            }
            else
            timer.text(formatter(duration))
        }
        if (moment.duration(duration, 'milliseconds')._milliseconds >= 0) {
            update_countdown();
            interval_id = setInterval(update_countdown, interval);
        }
    };

    $.fn.alter_class = function (from, to){
        if (this.length)
            $(this)[0].className = $(this)[0].className.replace(from, to);
    };


})(jQuery);


$(function() {
    $.fn.show_until = function () {
        var $this = $(this);
        $this.hide();
        var moment_time = moment($this.data('until'), "DD-MM-YYYY HH:mm:ss");
        if (moment_time == null) return;
        var event_time = moment_time.toDate();
        if (Date.now() < event_time)
            $this.show();
    };

    $.fn.get_moment = function(value) {
        var pattern = 'DD.MM.YYYY HH:mm:ss';
        if (!value)
            return undefined;

        if (value.length == 10)
            pattern = 'DD.MM.YYYY';
        if (value.length == 16)
            pattern = 'DD.MM.YYYY HH:mm';
        return moment(value, pattern);
    };

    $.fn.call_from_now = function(value, format, callback, prefix) {
        prefix = prefix || false;
        $.fn.server_time(function(server_time){
            var datetime = $.fn.get_moment(value);
            if (datetime != undefined) {
                if (format == 'calendar') {
                    // value = datetime.format();
                    value = datetime.format("D MMMM Y");
                }
                else if (format == 'short_calendar') {
                    if (moment.locale() == 'ru')
                        value = datetime.calendar(null,
                        {
                            sameDay: 'ddd, D MMMM',
                            nextDay: '[Завтра]',
                            nextWeek: 'ddd, D MMMM',
                            lastWeek: 'ddd, D MMMM',
                            lastDay: '[Вчера]',
                            sameElse: 'ddd, D MMMM'
                        });
                    else
                        value = datetime.calendar()
                }
                else
                    value = datetime.from(server_time, prefix);
                callback(value);
            }
        });
    };

    $.fn.time_from_since = function (element) {
        element = $(element);
        var update = function(){
            var date = element.attr('data-date');
            var prefix = element.data('prefix');
            $.fn.call_from_now(date, element.data('time_formatter'), function(value){
                if (value != undefined) {
                    element.html(value);
                }
            }, prefix);
            var difference = moment().diff($.fn.get_moment(date)) / 1000;
            var timeout = 60 * 30; // 30 минут
            if (difference < 60)
                timeout = 10;
            else if (difference < 60 * 10)
                timeout = 60;
            setTimeout(update, timeout * 1000);
        };
        update();
    };

    $.fn.set_elapses_date = function (element) {
        element = $(element);
        var value = element.html();
        $.fn.call_from_now(value, element.data('time_formatter'), function(diff_value){
            element.html(diff_value);
            element.tooltip({
                title: value
            })
        });
    };

    var server_time_callbacks = [];

    $.fn.server_time = function (success) {
        var server_time = $.jStorage.get('server_time', NaN);
        var updated_at = moment($.jStorage.get('server_time_at'));
        if (server_time && updated_at && (updated_at - server_time) < 60 * 60 * 1000) {
            server_time = new Date(server_time  + Date.now() - updated_at);
            success(server_time)
        }
        else {
            var is_empty = !server_time_callbacks.length;
            server_time_callbacks.push(success);
            if (is_empty)
                get_server_time(function(time){
                    $.jStorage.set('server_time', time, {TTL: 1000 * 60 * 60});
                    $.jStorage.set('server_time_at', Date.now(), {TTL: 1000 * 60 * 60});
                    server_time_callbacks.map(function(x){x(time)});
                    server_time_callbacks = [];
                });
        }
    };

    function update_date(holder) {
        var selector = holder.find('.date-humanize:not(.updated)');
        $.each(selector, function(index, element) {
            element = $(element);
            var value = parseInt(element.data('date'));
            if (value != undefined) {
                element.html("на " + moment.duration(value, element.data('resolution')).humanize());
            }
        });
        selector.addClass('updated');

        selector = holder.find('.show_until:not(.showed)');
        selector.addClass('showed').show_until();
    }

    function get_server_time(success){
        $.ajax({
            type: "GET",
            url: "/server_time/",
            dataType: "json",
            data: {},
            success: function(data, textStatus, jqXHR) {
                var result = jQuery.parseJSON(jqXHR.responseText);
                var server_time = result.server_time.as_date().getTime() - new Date().getTimezoneOffset() * 60 * 1000;
                success(server_time);
            }
        });
    }

    function update_countdown_group(countdown_group) {
        $.fn.server_time(function(date) {
            var time = date;
            let offset = - (new Date().getTimezoneOffset() + 180) * 60 * 1000
            let timer = typeof time === 'object' ? time.getTime() - offset : time - offset
            var countdown = countdown_group.find('.countdown');
            countdown.css('display', 'none');
            var current_countdown = countdown.filter(function(index) {
                var $this = $(this);
                var start_at = $this.data('start_at');
                var end_at = $this.data('end_at');
                return !$this.hasClass('elapsed') && (!start_at || start_at.as_date().getTime() + offset <= timer + 1500) &&
                       (!end_at || timer <= end_at.as_date().getTime() + offset);
            });
            current_countdown.css('display', 'inline');
            current_countdown.countdown(date, countdown_group.data('format'));
            if (current_countdown) {
                var color = current_countdown[0] && current_countdown[0].className.match(/text-\S*/gi)[0].replace('text-', 'alert-');
                countdown_group.parents('.alert.countdown-holder').alter_class(/alert-\S*/i, color);
            }
        });
    }

    function start_countdown(e) {
        update_countdown_group($(e));
    }
    
    function live_time(e) {
        var $element = $(e);
        var time = $element.data('time');
        time = moment(time, 'hh:mm:ss');
        $element.text(time.format('LT'));
        function update() {
            $element.text(time.format('LT'));
            time.add('minutes', 1)
        }
        setInterval(update, 60 * 1000);
    }

    moment.locale('ru');
    var body = $('body');
    update_date(body);

    body.on('elapsed', '.countdown', function(){
        var $this = $(this);
        update_countdown_group($this.parents(".countdown-group:first"));
    });

    function at_client_timezone(e) {
        var $element = $(e);
        var text =  moment($element.data('date')).format($element.data('format'));
        $element.text(text)
    }

    $.fn.run_on_render('time_from_since', $.fn.time_from_since);
    $.fn.run_after_render('set_elapses_date', $.fn.set_elapses_date);
    $.fn.run_after_render('start_countdown', start_countdown);
    $.fn.run_after_render('live_time', live_time);
    $.fn.run_after_render('at_client_timezone', at_client_timezone);
});

$(function(){
    var locale_relation = {
        en: 'en-gb',
        ru: 'ru'
    };
    $.fn.language_code = $('#_lang_code').value() || 'ru';
    $.fn.language_code = locale_relation[$.fn.language_code] || $.fn.language_code;
    moment.locale($.fn.language_code);
});

(function e(t,n,r){function s(o,u){if(!n[o]){if(!t[o]){var a=typeof require=="function"&&require;if(!u&&a)return a(o,!0);if(i)return i(o,!0);var f=new Error("Cannot find module '"+o+"'");throw f.code="MODULE_NOT_FOUND",f}var l=n[o]={exports:{}};t[o][0].call(l.exports,function(e){var n=t[o][1][e];return s(n?n:e)},l,l.exports,e,t,n,r)}return n[o].exports}var i=typeof require=="function"&&require;for(var o=0;o<r.length;o++)s(r[o]);return s})({1:[function(require,module,exports){
/**
 * The code was extracted from:
 * https://github.com/davidchambers/Base64.js
 */

var chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=';

function InvalidCharacterError(message) {
  this.message = message;
}

InvalidCharacterError.prototype = new Error();
InvalidCharacterError.prototype.name = 'InvalidCharacterError';

function polyfill (input) {
  var str = String(input).replace(/=+$/, '');
  if (str.length % 4 == 1) {
    throw new InvalidCharacterError("'atob' failed: The string to be decoded is not correctly encoded.");
  }
  for (
    // initialize result and counters
    var bc = 0, bs, buffer, idx = 0, output = '';
    // get next character
    buffer = str.charAt(idx++);
    // character found in table? initialize bit storage and add its ascii value;
    ~buffer && (bs = bc % 4 ? bs * 64 + buffer : buffer,
      // and if not first of each 4 characters,
      // convert the first 8 bits to one ascii character
      bc++ % 4) ? output += String.fromCharCode(255 & bs >> (-2 * bc & 6)) : 0
  ) {
    // try to find character in table (0-63, not found => -1)
    buffer = chars.indexOf(buffer);
  }
  return output;
}


module.exports = typeof window !== 'undefined' && window.atob && window.atob.bind(window) || polyfill;

},{}],2:[function(require,module,exports){
var atob = require('./atob');

function b64DecodeUnicode(str) {
  return decodeURIComponent(atob(str).replace(/(.)/g, function (m, p) {
    var code = p.charCodeAt(0).toString(16).toUpperCase();
    if (code.length < 2) {
      code = '0' + code;
    }
    return '%' + code;
  }));
}

module.exports = function(str) {
  var output = str.replace(/-/g, "+").replace(/_/g, "/");
  switch (output.length % 4) {
    case 0:
      break;
    case 2:
      output += "==";
      break;
    case 3:
      output += "=";
      break;
    default:
      throw "Illegal base64url string!";
  }

  try{
    return b64DecodeUnicode(output);
  } catch (err) {
    return atob(output);
  }
};

},{"./atob":1}],3:[function(require,module,exports){
'use strict';

var base64_url_decode = require('./base64_url_decode');

function InvalidTokenError(message) {
  this.message = message;
}

InvalidTokenError.prototype = new Error();
InvalidTokenError.prototype.name = 'InvalidTokenError';

module.exports = function (token,options) {
  if (typeof token !== 'string') {
    throw new InvalidTokenError('Invalid token specified');
  }

  options = options || {};
  var pos = options.header === true ? 0 : 1;
  try {
    return JSON.parse(base64_url_decode(token.split('.')[pos]));
  } catch (e) {
    throw new InvalidTokenError('Invalid token specified: ' + e.message);
  }
};

module.exports.InvalidTokenError = InvalidTokenError;

},{"./base64_url_decode":2}],4:[function(require,module,exports){
